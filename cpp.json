			"        if (nodes[i]) {",
			"            int left = 2 * i + 1;",
			"            int right = 2 * i + 2;",
			"            if (left < arr.size()) nodes[i]->left = nodes[left];",
			"            if (right < arr.size()) nodes[i]->right = nodes[right];",
			"        }",
			"    }",
			"    return nodes[0];",
			"}",
