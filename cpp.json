			"};",
			"",
			"TreeNode* buildTree(const vector<int>& arr) {",
			"    if (arr.empty()) return nullptr;",
			"    vector<TreeNode*> nodes(arr.size(), nullptr);",
			"    for (int i = 0; i < arr.size(); i++) {",
			"        if (arr[i] != -1) nodes[i] = new TreeNode(arr[i]);",
			"    }",
